#include <stdio.h>
#include "../../utils/node.h"

struct node {
  int value;
  node* next;
};

node* stack_create() {

  node* root = NULL;

  return root;

}  

void stack_push(node* root, node* item) {
  
  node* aux = root;
  
  root = item;
  
  printf("%d\n", root->value);

  item->next = aux; 
  
}

node* stack_pop(node* root) {

  node* aux = root;

  if(aux->next) {

    root = aux->next;

  } else {

    return NULL;
    
  }  

  return aux;

}
