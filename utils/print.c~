#include <stdio.h>
#include "print.h"
#include "node.h"

struct node {
  int value;
  node* next;
};

void print_structure(node* root) {
  
  node* aux = root;
  
  if(aux == NULL) {
    printf("asd");
  }

  printf("[");

  while(aux) {
    
    printf("%d", aux->value);
    
    if(aux->next) {
      printf(", ");
    }

    aux = aux->next;
    
  }

  printf("]\n");

}
